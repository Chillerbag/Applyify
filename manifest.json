{
    "manifest_version": 3,
    "name": "Applyify",
    "description": "Scrapes LinkedIn job posting data",
    "version": "0.1",
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png" 
    },
    "content_scripts": [
      {
        "js": ["scripts/scraper.js"],
        "matches": ["https://www.linkedin.com/jobs/collections/*?currentJobId=*"]
      }
    ]
  }